  "name": "ember-cheat-sheet",
  "version": "1.0.0",
  "private": true,
  "description": "Cheatsheets for Ember",
  "repository": {
    "type": "git",
    "url": "https://github.com/NullVoxPopuli/ember-cheat-sheet.git"
